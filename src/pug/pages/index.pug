block content
    .wrapper
        include ../modules/header/header
        .content
            block content
    main.main
        section.hero
            .container
                .hero__wrap
                    .hero__banner
                        .wow.fadeInLeft.hero__banner-img
                            img.banner-img(src="./static/img/face.png")
                        .wow.fadeInRight.hero__content
                            h1.hero__content-heading
                                span.hero__content-name Кошман Андрей
                                span.hero__content-prof Front-End developer
                            h3.hero__content-info Телефон:
                                span
                                    a.hero__content-number(href="tel:+380732195862") +380732195862
                            h3.hero__content-info Email:
                                span
                                    a.hero__content-mail(href="mailto:teplandrey41@gmail.com") teplandrey41@gmail.com
                            h3.hero__content-info Дата рождения:
